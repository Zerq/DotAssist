{
  "version": "1.0.0",
  "name": "asp.net",
  "private": true,
  "scripts": {
    "build": "tsc && cd src &&  npm run copyCss && npm run copyHtml",
    "fullbuild": "tsc && npm run copyPng && npm run copyCss && npm run copyHtml",
    "copyPng": "cd src && copyfiles -s ./**/*.png ../wwwroot",
    "copyCss": "cd src && copyfiles ./**/*.css ../wwwroot",
    "copyHtml": "cd src && copyfiles ./**/*.html ../wwwroot"
  },
  "devDependencies": {
    "copyfiles": "2.4.1",
    "eslint": "^8.57.0",
    "path": "0.12.7",
    "typescript": "5.3.3"
  }
}

